local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()

local Window = Library.CreateLib("üåÉ True Scripts | !True#0001 üåÉ", colors)

local colors = {
   SchemeColor = Color3.fromRGB(255, 0, 0),
   Background = Color3.fromRGB(0, 0, 0),
   Header = Color3.fromRGB(85, 255, 127),
   TextColor = Color3.fromRGB(255,255,255),
   ElementColor = Color3.fromRGB(0, 0, 0)
}


--Home Tab

local Home = Window:NewTab("Home")
local HomeSection = Home:NewSection("‚≠ê{.gg/FwJvrN9J84}‚≠ê")

HomeSection:NewLabel("This GUI is made by !True#0001")
HomeSection:NewLabel("Updating regularly with new scripts!")


--Universal Tab

local Main = Window:NewTab("Universal")
local MainSection = Main:NewSection("Universal GUI'S")

MainSection:NewButton("Infinite yield", "Executes Infinite yield.", function()
   loadstring(game:HttpGet('https://raw.githubusercontent.com/EdgeIY/infiniteyield/master/source'))()
end)

MainSection:NewToggle("Walkspeed On/Off", "Toggles walkspeed", function(state)
   if state then
      game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = 100
   else
      game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = 16
   end
end)


--Da hood tab

